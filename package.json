{
  "name": "shuttle-signups",
  "version": "0.0.0",
  "description": "A web interface to simplify the process of signing up for special shuttles at RPI.",
  "main": "app.js",
  "scripts": {
    "start": "concurrently \"mongod --dbpath=data --smallfiles\" \"tsc --project ./web/\" \"tsc --project ./web/ -w\" \"npm run build-css\" \"nodemon app.js\" ",
    "start-nomongo": "concurrently \"tsc --project ./web/\"  \"tsc --project ./web/ -w\" \"npm run build-css\" \"NODE_ENV=development nodemon app.js\"",
    "build-css": "concurrently \"node-sass web/assets/scss/ -o web/assets/css/\" \"node-sass -w web/assets/scss/ -o web/assets/css/\"",
    "start-app-only": "node app.js",
    "test": "gulp"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/wtg/shuttle-signups.git"
  },
  "author": "RPI Web Technologies Group",
  "license": "BSD-2-Clause",
  "bugs": {
    "url": "https://github.com/wtg/shuttle-signups/issues"
  },
  "homepage": "https://github.com/wtg/shuttle-signups#readme",
  "dependencies": {
    "@angular/common": "*",
    "@angular/compiler": "*",
    "@angular/core": "*",
    "@angular/forms": "*",
    "@angular/http": "*",
    "@angular/platform-browser": "*",
    "@angular/platform-browser-dynamic": "*",
    "@angular/router": "*",
    "@angular/upgrade": "*",
    "body-parser": "*",
    "bootstrap": "*",
    "cas-authentication": "*",
    "connect-mongo": "*",
    "core-js": "*",
    "es6-shim": "*",
    "express": "*",
    "express-session": "*",
    "express-ws": "*",
    "handlebars": "^4.0.10",
    "material-design-lite": "*",
    "moment": "^2.18.1",
    "mongoose": "4.10.0",
    "node-sass": "*",
    "nodemailer": "^4.4.0",
    "nodemon": "*",
    "q": "*",
    "reflect-metadata": "*",
    "rxjs": "*",
    "serve-favicon": "*",
    "systemjs": "*",
    "v9": "^1.0.8",
    "zone.js": "*"
  },
  "devDependencies": {
    "child_process": "^1.0.2",
    "concurrently": "^3.0.0",
    "gulp": "^3.9.1",
    "gulp-mocha": "^3.0.1",
    "gulp-nodemon": "^2.2.1",
    "mocha": "*",
    "run-sequence": "*",
    "supertest": "*",
    "typescript": "*"
  }
}
